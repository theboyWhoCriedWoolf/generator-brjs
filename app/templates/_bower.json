{
  "name": "<%= _.slugify(appname) %>",
  "componentsDirectory": "app/bower_components",
  "private": true,
  "dependencies": {<% if (includeBootstrap) { if (includeCompass) { %>
    "bootstrap-sass-official": "~3.1.0",<% } else { %>
    "bootstrap": "~3.0.3",<% }} if (includeModernizr) { %>
    "modernizr": "~2.6.2",<% } %>
    "jquery"          : "latest",
    "requirejs"       : "latest",
    "handlebars"      : "latest",
    "breakpoint-sass" : "latest",
    "lodash"          : "latest",
    "backbone"        : "latest",
    "require-handlebars-plugin" : "latest",
    "requirejs-domready" : "latest"
  },
  "devDependencies": {}
}